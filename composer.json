{
    "name": "jmickela/rootid-cli",
    "description": "Simple CLI to automate development workflows at Rootid.com",
    "authors": [
        {
            "name": "Jason Neumark Mickela",
            "email": "jason@rootid.com"
        }
    ],
    "require": {
        "consolidation/robo": "^1",
        "pantheon-systems/terminus": "^2"
